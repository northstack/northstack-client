{
    "name": "northstack/northstack-client",
    "description": "Commands for serverless environment management",
    "type": "library",
    "license": "GPL-3.0-only",
    "bin": [
        "bin/northstack"
    ],
    "autoload": {
        "psr-4": {
            "NorthStack\\NorthStackClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "rdlowrey/auryn": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "vlucas/phpdotenv": "^3.6",
        "monolog/monolog": "^1.21",
        "namshi/cuzzle": "^2.0",
        "ramsey/uuid": "^3.5",
        "colinodell/json5": "^1.0",
        "eloquent/enumeration": "^5.1",
        "ratchet/pawl": "^0.3.2",
        "giggsey/libphonenumber-for-php": "^8.9",
        "docker-php/docker-php": "^2.0",
        "alchemy/zippy": "dev-master",
        "equip/data": "^2.4",
        "symfony/console": "^4.3",
        "jane-php/open-api-runtime": "4.2.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^8.1"
    }
}
